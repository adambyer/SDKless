{
	"global": {
		"merge": {
			"REDIRECT-URI": "****"
		}
	},
	"endpoints": {
		"pull_import_lists": {
			"maps_to": "lists_list",
			"parameters": {
				"limit": 2,
				"sort_field": "created",
				"sort_dir": "ASC"
			},
			"output": {
				"data": {
					"location": ["data"],
					"items": {
						"locations": {
							"id": "id",
							"name": "name"
						}
					}
				}
			}
		},
		"pull_contacts_by_email": {
			"maps_to": "search_members",
			"parameter_maps": {
				"email_address": "query"
			}
		},
		"pull_list_contacts": {
			"maps_to": "export_list",
			"parameter_maps": {
				"list_id": "id"
			},
			"output": {
				"data": {
					"items": {
						"locations": {
							"email_address": "Email Address",
							"first_name": "First Name",
							"last_name": "Last Name"
						}
					}
				}
			}
		},
		"pull_list_segments": {
			"maps_to": "list_segments",
			"parameter_maps": {
				"list_id": "id"
			},
			"output": {
				"data": {
					"location": ["static"]
				}
			}
		},
		"push_list_segment": {
			"maps_to": "list_static_segment_add",
			"parameter_maps": {
				"list_id": "id",
				"segment_name": "name"
			},
			"output": {
				"data": {
					"location": ["id"],
					"format": "scalar"
				}
			}
		},
		"push_list_contacts": {
			"maps_to": "list_batch_subscribe",
			"parameters": {
				"double_optin": false,
				"update_existing": false,
				"replace_interests": true
			},
			"parameter_maps": {
				"list_id": "id",
				"contacts": "batch",
				"email_address": "email",
				"first_name": "FNAME",
				"last_name": "LNAME"
			},
			"array_set_templates": {
				"contacts": {
					"email": {
						"email": "email_address"
					},
					"merge_vars": {
						"FNAME": "first_name",
						"LNAME": "last_name"
					}
				}
			}
		},
		"push_segment_contacts": {
			"maps_to": "list_static_segment_members_add",
			"parameter_maps": {
				"list_id": "id",
				"segment_id": "seg_id",
				"contacts": "batch"
			},
			"array_set_templates": {
				"contacts": {
					"email": "email_address"
				}
			}
		}
	}
}