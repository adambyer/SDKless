{
	"base_uri": "https://*|DOMAIN|*/admin/api.php",
	"common_endpoint_settings": {
		"all": {
			"method": "post",
			"output_format": "json",
			"parameters": {
				"api_key": "*|API-KEY|*"
			},
			"curl_options": {
				"HEADER": false,
				"RETURNTRANSFER": true,
				"SSL_VERIFYPEER": false,
				"FOLLOWLOCATION": true
			}
		},
		"get": {
			"parameters": {
				"api_output": "json"
			}
		}
	},
	"endpoints": {
		"list_list": {
			"uri": null,
			"method": "get",
			"parameters": {
				"api_action": "list_list",
				"ids": "all"
			}
		},
		"contact_list": {
			"uri": null,
			"method": "get",
			"parameters": {
				"api_action": "contact_list",
				"filters[listid]": null
			},
			"paging": {
				"type": "page_number",
				"parameters": {
					"page_number": {
						"name": "page"
					}
				}
			}
		}
	}
}